{
    "Initial plugin set": {
        "prefix": "!rmmvStart",
        "body": [
            "$BLOCK_COMMENT_START:",
            " * @plugindesc ${1:Description}",
            " * @author ${2:Author}",
            " * ",
            " * @help",
            " * ",
            " $BLOCK_COMMENT_END"
        ],
        "description": "creates the initial set of information and functions"
    },
    "Code Block Comment": {
        "prefix": "!rmmvComment",
        "body": [
            "$LINE_COMMENT-----------------------------------------------------------------------------",
            "$LINE_COMMENT ${1:Name}",
            "$LINE_COMMENT",
            "$LINE_COMMENT ${2:Description}"
        ],
        "description": "creates a comment block to make code cleaner and easier to find"
    },
    "Plugin Manager Parameter": {
        "prefix": "PluginManager.parameters('')",
        "body": [
            "PluginManager.parameters('${1:Name}')"
        ],
        "description": "creates a PluginManager.parameters code"
    },
    "MVstructContent": {
        "prefix": "!rmmvStructContent",
        "body": [
            "$BLOCK_COMMENT_START:~struct~${1:StructName}:",
            " * ",
            " * ",
            " $BLOCK_COMMENT_END"
        ],
        "description": "creates the initial struct"
    },
    "MVNote": {
        "prefix": "@type note",
        "body": [
            "@param ${1:Parameters}",
            "* @desc ${2:Description}",
            "* @type note",
            "* @default ${3:Default}"
        ],
        "description": "creates a multi-line string function"
    },
    "MVparent": {
        "prefix": "!rmmvParent",
        "body": [
            "* @parent ${1:Parent}"
        ],
        "description": "create @parent"
    },
    "MVselectbox": {
        "prefix": "@type select",
        "body": [
            "@param ${1:Parameters}",
            "* @desc ${2:Description}",
            "* @type select",
            "* @default ${3:Default}",
            "* @option",
            "* @value",
            "* @option",
            "* @value"
        ],
        "description": "create a select function"
    },
    "MVoptionandvalue": {
        "prefix": "@value",
        "body": [
            " @option",
            "* @value"
        ],
        "description": "create a option with value function"
    },
